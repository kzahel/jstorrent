{
  "manifest_version": 3,
  "name": "JSTorrent",
  "version": "0.0.6",
  "description": "BitTorrent client for Chrome. Download torrents directly from your browser.",
  "permissions": ["nativeMessaging", "storage", "notifications", "unlimitedStorage"],
  "optional_permissions": ["power"],
  "background": {
    "service_worker": "sw.js",
    "type": "module"
  },
  "action": {},
  "icons": {
    "16": "icons/js-16.png",
    "32": "icons/js-32.png",
    "128": "icons/js-128.png"
  },
  "host_permissions": ["http://100.115.92.2/*"],
  "externally_connectable": {
    "matches": [
      "https://new.jstorrent.com/*",
      "https://jstorrent.com/*",
      "http://local.jstorrent.com/*"
    ]
  }
}
