{
  "manifest_version": 3,
  "name": "JSTorrent Extension",
  "version": "0.0.1",
  "description": "JSTorrent MV3 Extension",
  "permissions": ["offscreen", "nativeMessaging", "storage"],
  "background": {
    "service_worker": "sw.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/ui/app.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  }
}
